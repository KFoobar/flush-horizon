{
    "name": "kfoobar/flush-horizon",
    "description": "A Laravel package designed to efficiently manage and clear failed jobs from Horizon.",
    "type": "plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "David Villa",
            "email": "david@kfoobar.se"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/console": ">=8.0",
        "illuminate/redis": ">=8.0",
        "illuminate/support": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "KFoobar\\Horizon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KFoobar\\Horizon\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
